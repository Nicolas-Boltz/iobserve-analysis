        this.lqnsUriPath = configuration.getStringProperty(ConfigurationKeys.LQNS_URI_PATH);
        this.deployablesFolderPath = configuration.getStringProperty(ConfigurationKeys.DEPLOYABLES_FOLDER_PATH);
       

